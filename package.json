{
  "name": "@biem/template-utils",
  "description": "Biem Template Utils",
  "version": "1.2.4",
  "author": "Michael Freund <michael@biem.ch>",
  "devDependencies": {
    "@biem/eslint-config": "^2.1.3",
    "@types/node": ">=18",
    "@types/nunjucks": "^3.2.6",
    "@types/react": "^18.3.1",
    "eslint": "^9.17.0",
    "glob": "^11.0.0",
    "globals": "^15.14.0",
    "nunjucks": "^3.2.4",
    "react": "^18.3.1",
    "rollup": "^4.29.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vite-plugin-dts": "^4.4.0"
  },
  "engines": {
    "node": ">=20.12.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./hook/*": {
      "types": "./dist/hook/*.d.ts",
      "default": "./dist/hook/*.js"
    },
    "./plugin/vite/*": {
      "types": "./dist/plugin/vite/*.d.ts",
      "default": "./dist/plugin/vite/*.js"
    },
    "./type/*": {
      "types": "./dist/type/*.d.ts",
      "default": "./dist/type/*.js"
    },
    "./util/*": {
      "types": "./dist/util/*.d.ts",
      "default": "./dist/util/*.js"
    }
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://github.com/biemch/biem-template-utils",
  "license": "MIT",
  "main": "./dist/index.js",
  "peerDependencies": {
    "nunjucks": "^3.2.4",
    "react": "^18.3.1",
    "vite": "^6.0.6"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "biemch/biem-template-utils.git",
  "scripts": {
    "build": "vite build",
    "dev": "vite dev",
    "format:pkg": "npx syncpack format",
    "lint": "eslint \"src/**/{*.ts,*.tsx}\"",
    "lint:fix": "eslint --fix \"src/**/{*.ts,*.tsx}\"",
    "prepublishOnly": "yarn build"
  },
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts"
}
